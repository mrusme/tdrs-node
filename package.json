{
  "name": "tdrs",
  "version": "1.0.1",
  "description": "TDRS Node.js module",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "cd ext/zyre/bindings/nodejs && npm install node-ninja bindings nan prebuild && PATH=./node_modules/.bin/:$PATH ./build.sh && cd ../../../../ && mkdir -p ./build && mv ./ext/zyre/bindings/nodejs/build/Release/zyre.node ./build/zyre.node && rm -rf ./ext/libzmq ./ext/czmq ./ext/zyre/bindings/nodejs/gyp-*",
    "flow": "flow",
    "compile": "gulp build",
    "prepublish": "npm run compile",
    "test": "npm run compile && ava -v",
    "SimpleClient": "node ./examples/SimpleClient.js",
    "DiscoveryClient": "node ./examples/DiscoveryClient.js"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/weltraumco/tdrs-node.git"
  },
  "keywords": [
    "tdrs",
    "weltraum",
    "zeromq",
    "event",
    "hub"
  ],
  "author": {
    "name": "Weltraum",
    "email": "houston@weltraum.co",
    "url": "https://weltraum.co"
  },
  "contributors": [
    {
      "name": "Marius M.",
      "email": "marius@twostairs.com",
      "url": "https://twostairs.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weltraumco/tdrs-node/issues"
  },
  "homepage": "https://github.com/weltraumco/tdrs-node#readme",
  "ava": {
    "require": "babel-register",
    "babel": "inherit",
    "files": [
      "src/test/**/*Test.js"
    ],
    "source": [
      "src/**/*.js",
      "!src/test/**/*"
    ]
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "eslint-plugin-ava": "^3.1.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "flow-bin": "^0.34.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^3.0.1",
    "gulp-jsdoc3": "^1.0.0",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-watch": "^4.3.10",
    "minami": "^1.1.1",
    "path": "^0.12.7",
    "rimraf": "^2.5.4",
    "run-sequence": "^1.2.2"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "node-cache": "^4.1.0",
    "node-uuid": "^1.4.7",
    "zmq": "^2.15.3"
  }
}
